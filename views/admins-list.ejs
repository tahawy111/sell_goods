<%- include('./partials/head.ejs') %>








<div class="container-fluid">
  <div class="row my-4">
    <div class="col-lg-12">
      <div class="table-responsive me-5">

        <table class="table table-striped text-center my-3">
          <thead>
            <tr class="table-dark">
              <th>#</th>
              <th>Name</th>
              <th>Username</th>
              <th>More</th>
            </tr>
          </thead>

          <tbody style="vertical-align: middle">
            <% data.forEach((ele,index)=>{ %>
            <tr>
              <td><%= index %></td>
              <td><%= ele.name %></td>
              <td><%= ele.username %></td>
                <td><a href="/edit-admin/<%= ele._id %>" class="btn btn-primary">Edit</a></td>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>








<%- include('./partials/footer.ejs') %>


