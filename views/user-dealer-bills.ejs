<%- include('./partials/head.ejs') %>

<div class="container-fluid">
  <h3 class="mt-2">قائمة فواتير: <%= data[0].userDealer.dealerName %></h3>
  <div class="row my-4">
    <div class="col-lg-12">
      <div class="table-responsive">
        <table
          class="table table-striped text-center my-3 table-data table-home"
        >
          <thead>
            <tr class="table-dark">
              <th>#</th>
              <th>طباعة</th>
              <th>التاريخ</th>
              <th>السعر الاجمالي</th>
              <th>الكمية</th>
              <th>رقم الفاتورة</th>
              <th>تمت بواسطة</th>
            </tr>
          </thead>

          <tbody style="vertical-align: middle">
            <% data.forEach((ele,index)=>{ %>
            <tr>
              <td><%= index + 1 %></td>
              <td>
                <a
                  class="btn btn-primary"
                  target="_blank"
                  href="/bills-list/print/<%= ele._id %>"
                  >طباعة</a
                >
              </td>
              <td><%= ele.createdAt.toISOString() %></td>
              <td><%= ele.totalPrice %></td>
              <td><%= ele.totalQuantity %></td>
              <td><%= ele.billNumber %></td>
              <td><%= ele.adminName %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<%- include('./partials/footer.ejs') %>
